datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Project {
  id        String   @id @default(uuid())
  name      String
  status    Status
  amount    Float
  createdAt DateTime @default(now())
}

enum Status {
  DRAFT
  PUBLISHED
  ARCHIVED
}